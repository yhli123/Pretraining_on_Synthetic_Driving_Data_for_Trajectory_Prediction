Pretrain:
CUDA_VISIBLE_DEVICES=1 nohup python3 src/run.py --seed 42 --argoverse --master_port 8240 --future_frame_num 30 --do_train \
--data_dir /mnt/hdd2/yiheng/Pretrain_Data_360k_Aug_Orig/ \
--num_train_epochs 10.0 --learning_rate 0.0001 --train_batch_size 256 \
--output_dir pretrain/models.densetnt.e1 --hidden_size 128 --use_map --core_num 16 --use_centerline --distributed_training 1 \
--other_params semantic_lane direction l1_loss goals_2D enhance_global_graph subdivide goal_scoring laneGCN point_sub_graph \
lane_scoring complete_traj complete_traj-3 pretrain=0.50 mask='mix' pt_visualize > pretrain/train.log 2>& 1 &

# For PT evaluation, add 'pt_visualize' in other_params.

Finetune:
CUDA_VISIBLE_DEVICES=1 nohup python3 src/run.py --seed 42 --argoverse --master_port 8228 --future_frame_num 30 --do_train \
--data_dir /mnt/hdd2/yiheng/Argoverse-Dataset-v1_1/train/data/ \
--num_train_epochs 16.0 --learning_rate 0.001 --train_batch_size 64 \
--output_dir epoch8/models.densetnt.e1 --hidden_size 128 --use_map --core_num 16 --use_centerline --distributed_training 1 \
--other_params semantic_lane direction l1_loss goals_2D enhance_global_graph subdivide goal_scoring laneGCN point_sub_graph \
lane_scoring complete_traj complete_traj-3 use_pt_model=8 > epoch8/train.log 2>& 1 &

Eval
CUDA_VISIBLE_DEVICES=1 nohup python3 src/run.py --seed 42 --argoverse --master_port 8228 --future_frame_num 30 --do_train \
--data_dir_for_val /mnt/hdd2/yiheng/Argoverse-Dataset-v1_1/val/data \
--model_recover_path epoch8/models.densetnt.e1/model_save/model.16.bin \
--output_dir epoch8/models.densetnt.e1 --hidden_size 128 --train_batch_size 64 --use_map --core_num 32 --use_centerline --distributed_training 1 \
--do_eval --eval_params optimization MRminFDE=0.0 cnt_sample=9 opti_time=0.1 \
--other_params semantic_lane direction l1_loss goals_2D enhance_global_graph subdivide goal_scoring laneGCN point_sub_graph \
lane_scoring complete_traj complete_traj-3 > epoch8/evalfde.log 2>& 1 &